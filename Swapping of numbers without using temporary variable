               //This program allows you to swap numbers without using a temporary variable
#include <stdio.h> //for standard input output
int main()
{
  int num1,num2;
  printf("Enter Two Numbers : ");
  scanf("%d %d",&num1,&num2); 
  //bitwise algorithm on swapping
  num1=num1^num2;  //(^)Xor bitwise operator
  num2=num1^num2;
  num1=num1^num2;
  printf("Swapped Numbers are : %d  %d",num1,num2);
  return 0;     //Happy Coding :)
}
